{
  "name": "lsp-gateway",
  "version": "0.0.1",
  "description": "A gateway that provides a single endpoint for symbol navigation across multiple open-source language servers",
  "main": "lib/index.js",
  "bin": {
    "lspg": "bin/lsp-gateway"
  },
  "scripts": {
    "postinstall": "node lib/index.js install-binary",
    "server": "./bin/lsp-gateway server",
    "diagnose": "./bin/lsp-gateway diagnose",
    "version": "./bin/lsp-gateway version"
  },
  "keywords": [
    "lsp",
    "language-server",
    "gateway",
    "symbol-navigation",
    "definition",
    "references",
    "go",
    "python",
    "typescript",
    "javascript",
    "java",
    "gopls",
    "pylsp",
    "jdtls"
  ],
  "author": "LSP Gateway Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsp-gateway/lsp-gateway.git"
  },
  "homepage": "https://github.com/lsp-gateway/lsp-gateway",
  "bugs": {
    "url": "https://github.com/lsp-gateway/lsp-gateway/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "files": [
    "lib/",
    "bin/",
    "config.yaml",
    "README.md"
  ],
  "dependencies": {},
  "devDependencies": {},
  "optionalDependencies": {
    "lsp-gateway-linux-x64": "^0.0.1",
    "lsp-gateway-darwin-x64": "^0.0.1",
    "lsp-gateway-darwin-arm64": "^0.0.1",
    "lsp-gateway-win32-x64": "^0.0.1"
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/lsp-gateway"
  },
  "type": "commonjs",
  "directories": {
    "lib": "lib",
    "bin": "bin"
  },
  "config": {
    "unsafe-perm": true
  }
}